{
  "name": "arekx/jsonql",
  "description": "JsonQL Rest interface framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Aleksandar Panic",
      "email": "arekusanda1@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "ArekX\\JsonQL\\": "src/",
      "App\\": "app/"
    },
    "files": [
      "src/Validation/functions.php"
    ]
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "rdlowrey/auryn": "^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5",
    "phpunit/php-code-coverage": "^6.1"
  },
  "scripts": {
    "test": "php vendor/bin/phpunit",
    "document": "php document.php",
    "coverage": "php vendor/bin/phpunit --coverage-html coverage --whitelist src"
  }
}
